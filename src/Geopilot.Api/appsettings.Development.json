{
  "Auth": {
    "Authority": "http://localhost:4011/realms/geopilot",
    "ClientAudience": "geopilot-client",
    "ApiAudience": "geopilot-api",
    "AuthorizationUrl": "http://localhost:4011/realms/geopilot/protocol/openid-connect/auth",
    "UserInfoUrl": "http://localhost:4011/realms/geopilot/protocol/openid-connect/userinfo",
    "TokenUrl": "http://localhost:4011/realms/geopilot/protocol/openid-connect/token",
    "ApiOrigin": "https://localhost:7443",
    "FullScope": "profile email openid geopilot.api",
