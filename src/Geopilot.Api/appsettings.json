{
  "ConnectionStrings": {
    "Context": "Server=localhost;Port=5432;Database=geopilot;User Id=HAPPYWALK;Password=SOMBERSPORK;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "stacBrowserRoute": {
        "ClusterId": "stacBrowserCluster",
        "Match": {
          "Path": "/browser/{**catch-all}"
        }
      },
      "mapserviceRoute": {
        "ClusterId": "mapserviceCluster",
        "AuthorizationPolicy": "Anonymous",
        "Match": {
          "Path": "/mapservice/{validationId}"
        },
        "Transforms": [
          {
            "PathSet": "/"
          },
          {
            "QueryValueParameter": "MAP",
            "Append": "/etc/qgisserver/validationWithID0000/DiffVisualisierung.qgz"
          },
          {
            "RequestHeader": "X-Qgis-Service-Url",
            "Set": "https://localhost:5173/mapservice/validationWithID0000"
          }
        ]
      }
    }
  },
  "Storage": {
    "UploadDirectory": "Uploads",
    "AssetsDirectory": "Persistent"
  }
}
